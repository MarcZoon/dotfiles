# .gitconfig from https://gitlab.com/m3361/dotfiles

[user]
	name = MarcZoon
	email = marc@famzoon.nl

[alias]
	tree-ol = log --oneline --graph --decorate --all
    tree = log --graph --decorate -all

[init]
	defaultBranch = main

[color "status"]
	added = green
	changed = yellow
	untracked = red

[url "https://github.com"]
	insteadOf = gh:
[url "https://gitlab.com"]
	insteadof = gl:
